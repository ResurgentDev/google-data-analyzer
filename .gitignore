# Ignore development documentation
devdocs/

# Ignore backup files
*.bak.py

# Ignore generated reports and visualizations
reports/*
!reports/.gitkeep
reports/visualizations/*
!reports/visualizations/.gitkeep

# Ignore data files (track folders, not content)
data/raw/*
!data/raw/.gitkeep
data/processed/parsed/*
!data/processed/parsed/.gitkeep
data/processed/cleaned/*
!data/processed/cleaned/.gitkeep

# Ignore virtual environments
venv/

# Ignore log files
logs/*
!logs/.gitkeep
